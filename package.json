{
  "name": "project-gov-uz-cabinet-repo",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --color",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"src/**/*.{json,ts,tsx,js,jsx,md,scss,css,html}\"",
    "prepare": "husky install",
    "build:local": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:prod": "tsc && vite build --mode production",
    "publish:local": "pnpm build:local && docker build --platform linux/amd64 -t project-gov-uz-cabinet-repo:local . && docker run -p 5173:80 project-gov-uz-cabinet-repo:local",
    "publish:staging": "pnpm build:staging && docker build --platform linux/amd64 -t registry.gitlab.com/csexplorer/project-gov-uz-cabinet-repo:latest . && docker push registry.gitlab.com/csexplorer/project-gov-uz-cabinet-repo:latest",
    "publish:prod": "pnpm build:prod && docker build --platform linux/amd64 -t registry.gitlab.com/csexplorer/project-gov-uz-cabinet-repo:latest-test . && docker push registry.gitlab.com/csexplorer/project-gov-uz-cabinet-repo:latest-test"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@hookform/resolvers": "^3.3.1",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/node": "^20.5.9",
    "antd": "^5.8.6",
    "axios": "^1.5.0",
    "axios-auth-refresh": "^3.3.6",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "query-string": "^8.1.0",
    "radash": "^11.0.0",
    "rc-menu": "^9.12.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.20.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.47.0",
    "react-imask": "^7.1.3",
    "react-outer-click": "^3.8.0",
    "react-paginate": "^8.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0",
    "sass": "^1.66.1",
    "store2": "^2.14.2",
    "use-debounce": "^10.0.0",
    "use-query-params": "^2.2.1",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/diff": "^5.0.7",
    "@types/react": "^18.2.15",
    "@types/react-datepicker": "^4.19.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.10.0",
    "storybook": "^7.4.6",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
